#include <stdio.h>
#include <stdlib.h>
#include <math.h> 

int main()
{
    int taban, us, sonuc;

    printf("Tabani girin: "); 
    scanf("%d", &taban);

    printf("Us girin: "); 
    scanf("%d", &us);

    sonuc = pow(taban, us);
    printf("%d uzeri %d = %d\n", taban, us, sonuc);

    return 0;
}
